{
    "name": "benchmark-web-server",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "bash": "copyfiles src/**/*.sh dist/ && copyfiles .env dist/",
        "prebuild": "tslint -c tslint.js -p tsconfig.json --fix",
        "build": "rm -rf dist && node_modules/typescript/bin/tsc -p tsconfig.json && yarn bash",
        "prestart": "yarn build",
        "start": "node dist/src/server.js",
        "test": "node node_modules/jest/bin/jest.js"
    },
    "dependencies": {
        "@elastic/elasticsearch": "6",
        "@google-cloud/compute": "^1.1.1",
        "@octokit/rest": "^16.28.2",
        "@types/elasticsearch": "^5.0.34",
        "@types/node": "^12.6.3",
        "body-parser": "^1.18.3",
        "child-process-promise": "^2.2.1",
        "connect-history-api-fallback": "^1.6.0",
        "cookie-session": "^1.3.3",
        "dotenv": "^8.0.0",
        "express": "^4.16.4",
        "express-graphql": "^0.7.1",
        "global": "^4.4.0",
        "graphql": "^14.1.1",
        "graphql-tools": "^4.0.4",
        "restify": "^8.3.3"
    },
    "devDependencies": {
        "@types/graphql": "^14.2.2",
        "copyfiles": "^2.1.1",
        "eslint": "5.3.0",
        "eslint-config-airbnb-base": "13.2.0",
        "eslint-plugin-import": "^2.17.2",
        "jest": "^23.6.0",
        "supertest": "^3.4.1",
        "tslint": "^5.18.0",
        "tslint-config-airbnb": "^5.11.1",
        "typescript": "^3.5.3"
    }
}
